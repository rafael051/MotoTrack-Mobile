{
  "expo": {
    "name": "MotoTrack-Mobile",
    "slug": "MotoTrack-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0EA5E9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rafael51.mototrackmobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#0EA5E9",
      "translucent": false,
      "barStyle": "light-content",
      "hidden": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "themeColor": "#0EA5E9"
    },
    "plugins": [
      "expo-router",
      ["expo-notifications", {}],
      "expo-localization"
    ],
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#0EA5E9",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "MotoTrack"
    },
    "extra": {
      "router": { "origin": false },
      "eas": { "projectId": "a00d5094-35cb-4a09-ba9a-fc050dc1f79a" }
    }
  }
}
